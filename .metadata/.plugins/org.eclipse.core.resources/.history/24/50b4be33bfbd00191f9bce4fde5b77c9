import java.util.*;
import java.lang.*;

public class PrintThreeNames {

	public static void main(String[] args) {
		
		Scanner x=new Scanner(System.in);
		
		System.out.println("Enter three names:");
		String str=x.nextLine();
		
		String s[]=str.split(" ");
		System.out.println(Arrays.toString(s));
		x.close();
	}

}
